        <activity android:name=".demos.simpleDemo.SimpleActivity" />
        <activity android:name=".demos.listDemo.MyListActivity" />
        <activity android:name=".demos.twowaydemo.TwoWayActivity" />
        <activity android:name=".demos.expression.ExpressionActivity" />
